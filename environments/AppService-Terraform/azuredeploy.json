{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "CleanUp TFRunner": {
            "type": "bool",
            "defaultValue": true
        },
        "_artifactsLocation": {
            "type": "string"
        },
        "_artifactsLocationSasToken": {
            "type": "securestring"
        }
    },
    "variables": {
    },
    "resources": [
        {
            "apiVersion": "2017-05-10",
            "name": "[concat(deployment().name, '-runner')]",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('_artifactsLocation'), '/azuredeploy-runner.json', parameters('_artifactsLocationSasToken'))]"
                },
                "parameters": {
                    "CleanUp TFRunner": {
                        "value": "[parameters('CleanUp TFRunner')]"
                    },
                    "TFRunner Identity": {
                        "reference": {
                            "keyVault": {
                                "id": "$(LabKeyVaultId)"
                            },
                            "secretName": "udmi-clientid"
                        }
                    },
                    "TFRunner ResourceId": {
                        "reference": {
                            "keyVault": {
                                "id": "$(LabKeyVaultId)"
                            },
                            "secretName": "udmi-resourceid"
                        }
                    },
                    "_artifactsLocation": {
                        "value": "[parameters('_artifactsLocation')]"
                    },
                    "_artifactsLocationSasToken": {
                        "type": "[parameters('_artifactsLocationSasToken')]"
                    }
                }
            }
        }
    ],
    "outputs": {}
}